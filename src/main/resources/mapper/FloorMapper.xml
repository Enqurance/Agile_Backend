<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backend.mapper.FloorMapper">

    <resultMap id="BaseResultMap" type="com.example.backend.domain.Floor">
            <id property="floorId" column="floor_id" jdbcType="INTEGER"/>
            <result property="floorContent" column="floor_content" jdbcType="VARCHAR"/>
            <result property="floorLayers" column="floor_layers" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="postId" column="post_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        floor_id,floor_content,floor_layers,
        user_id,post_id
    </sql>
</mapper>
